{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "paths": {
    "@/*": ["src/*"],
  },
  "include": [
    "src/renderer/env.d.ts",
    "src/renderer/**/*",
    "src/renderer/**/*.vue",
    "src/preload/*.d.ts",
     "src/common/**/*"
, "src/renderer/stores", "src/renderer/router", "src/renderer/App.vue", "src/renderer/main.ts"  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
    }
  }
}
